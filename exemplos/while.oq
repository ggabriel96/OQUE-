fn square(x) {
    return x * x
}

fn main() {
    let i: int
    let x: int

    i = 0
    repetix (i < 10) {
        println(i: $i$, i^2: $square(i)$)
        x[i] = square(i)
        i = i + 1
    }
    println()

    i = 0
    repetix (i < 10) {
        println(x[i]: $x[i]$)
        i = i + 1
    }
}
